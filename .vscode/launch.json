{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch SshAgentEcho.Cli",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build:cli",
            "program": "${workspaceFolder}/src/SshAgentEcho.Cli/bin/Debug/net10.0/ssh-agent-echo.dll",
            "args": [
                "--print",
                "--sync",
                "--force"
            ],
            "cwd": "${workspaceFolder}/src/SshAgentEcho.Cli",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "Launch SshAgentEcho.Gui",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build:gui",
            "program": "${workspaceFolder}/src/SshAgentEcho.Gui/bin/Debug/net10.0/ssh-agent-echo-gui.dll",
            "cwd": "${workspaceFolder}/src/SshAgentEcho.Gui",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}